# converted to Fahrenheit 
c = 30
f = (c * 9/5) + 32
print(f)

# converted to Celsuis
f = 95
c = (f - 32) * 5/9
print(c) 

# converted from Kelvin to Celsuis
k = 310
c = k - 273.15
print(c)

# finding the average
four_day_temperature = [22, 28, 26, 24]
average = sum(four_day_temperature) / len(four_day_temperature)
print(average)

# finding the diffenece
a = 18
b = 33
difference = b - a
print(difference)

# finding the percentage
new = 35 
old = 20
percentage_increase = (new - old) / old * 100
print(percentage_increase)

# convert to Kelvin Process
f = 100
c = (f - 32) * 5/9
print(c)
k = c + 273.15
print(k)